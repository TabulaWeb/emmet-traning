{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "#/*": ["client/*/main"],
      "$/*": ["server/*/main"],
      "@/*": ["libs/*/main"],
      "~/*": ["public/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "client/pages/index/main.jsx"],
  "exclude": ["node_modules"]
}